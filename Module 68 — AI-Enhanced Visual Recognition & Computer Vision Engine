class VisionEngine:
    def __init__(self):
        self.detected_objects = []

    def analyze_image(self, image):
        objects = ["person", "car", "tree"]  # placeholder detection
        self.detected_objects.extend(objects)
        return objects

    def analyze_video(self, video_frames):
        results = []
        for frame in video_frames:
            results.append(self.analyze_image(frame))
        return results

# Example usage:
# engine = VisionEngine()
# image_results = engine.analyze_image("frame1.jpg")
# video_results = engine.analyze_video(["frame1.jpg","frame2.jpg"])
